{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "max-len": [1, 200],
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "linebreak-style": [0, "windows"],
    "no-mixed-operators": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0
  }
}
